<%- include ("header") -%>

    <div class="box" id="heading">
        <h1>  <%= listTitle %>    </h1>
    </div>

<div class="box" id="">
        <% for (var i=0 ; i < tasks.length ;i++){ %>
            <div class="item">
                <input type="checkbox" id="">
               <p> <%= tasks[i] %>  </p>
            </div>
            <% } %>
  
            
    <form action="/" class="item" method="post">
        <input type="text" name="newTask" autofocus placeholder="What do you want to do?" autocomplete="off">
        <button name="list"  type="submit" value=<%= listTitle %> >+</button>
    </form>
</div>
    

<%- include ("footer") -%> 




